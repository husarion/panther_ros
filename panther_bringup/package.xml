<?xml version="1.0"?>
<package format="3">
  <name>panther_bringup</name>
  <version>1.0.41</version>

  <description>Default launch files and configuration used to start Husarion Panther robot</description>
  <license>Apache License 2.0</license>

  <author email="dawid.kmak@husarion.com">Dawid Kmak</author>
  <author email="krzysztof.wojciechowski@husarion.com">Krzysztof Wojciechowski</author>
  <author email="pawel.kowalski@husarion.com">Pawel Kowalski</author>
  <maintainer email="support@husarion.com">Husarion</maintainer>

  <url type="website">https://husarion.com/</url>
  <url type="repository">https://github.com/husarion/panther_ros</url>
  <url type="bugtracker">https://github.com/husarion/panther_ros/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>imu_filter_madgwick</depend>
  <depend>panther_battery</depend>
  <depend>panther_description</depend>
  <depend>panther_driver</depend>
  <depend>panther_lights</depend>
  <depend>panther_manager</depend>
  <depend>panther_msgs</depend>
  <depend>panther_power_control</depend>
  <depend>phidgets_spatial</depend>
  <depend>robot_localization</depend>

  <!-- Python dependencies -->
  <depend>python3-click</depend>

</package>